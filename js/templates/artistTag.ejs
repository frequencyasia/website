<section class="c-content">
  <div class="row">
    <div class="col">
      <% if (data !== undefined){ %>
        <h1><a class='u-no-border' href="#wiki">Wiki</a> &rsaquo; <a class='u-no-border' href="#wiki/<%= type %>"><%= typePretty %></a> &rsaquo; <%= data.name %></h1>
        <h2 id="location-data"></h2>
        <% for(var i = 0; i < data.episodes.length; i++) { %>
          <article class="c-episode">
            <div class="c-episode__content">
              <% if (data.episodes[i].mixcloud_link && data.episodes[i].mixcloud_link.length) { %>
                <div class="c-episode__content__play js-play-episode" data-mixcloud="<%= data.episodes[i].mixcloud_link %>">
                  <span class="icon-play2"></span>
                </div>
              <% } %>
              <div class="c-episode__content__info">
                <h1 class="c-episode__content__title"><a href="#shows/<%= data.episodes[i].show_slug %>"><%= data.episodes[i].name %></a></h1>
                <p><%= data.episodes[i].tagline %></p>
                <p class="c-episode__content__date"><%= data.episodes[i].date %></p>
                <p class="c-episode__content__tags">
                  <% for (var j = 0; j < data.episodes[i].countries.length; j++) { %>
                    <a href="#wiki/countries/<%= data.episodes[i].countries[j].slug %>"><%= data.episodes[i].countries[j].name %></a>&nbsp;/&nbsp;
                  <% } %>
                  <% for (var j = 0; j < data.episodes[i].cities.length; j++) { %>
                    <a href="#wiki/cities/<%= data.episodes[i].cities[j].slug %>"><%= data.episodes[i].cities[j].name %></a>&nbsp;/&nbsp;
                  <% } %>
                  <% for (var j = 0; j < data.episodes[i].artists.length; j++) { %>
                    <a href="#wiki/artists/<%= data.episodes[i].artists[j].slug %>"><%= data.episodes[i].artists[j].name %></a>
                    <% if(j !=  data.episodes[i].artists.length - 1){ %>
                      &nbsp;/&nbsp;
                    <% } %>
                  <% } %>
                </p>
              </div>
              <% if(data.episodes[i].episode_image) { %>
                <img class="c-episode__content__image" src='/static/files/<%= data.episodes[i].episode_image %>' alt='<%= data.episodes[i].name %>'/>
              <% } %>
            </div>
            <div class="c-episode__description-toggle">+ More Info</div>
            <div class="c-episode__description"><%= data.episodes[i].description %></div>
          </article>
        <% } %>
      <% } %>
    </div>
  </div>
</section>