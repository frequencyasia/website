<% for (var i = 0; i < data.length; i++) { %>
    <a href="#shows/<%= data[i].slug %>" class='post-module'>
      <div class='thumbnail'>
        <img src='/static/files/<%= data[i].imagePath %>' alt='<%= data[i].name %>'>
      </div>
      <div class='post-content'>
        <h1 class='title'><%= data[i].name %></h1>
        <h2 class='sub_title'><%= data[i].frequency %></h2>
        <p class='description'><%= data[i].description %></p>
        <div class='post-meta'>
          <span class='timestamp'>
            <% if (data[i].num_episodes == 1) { %>
              1 Episode
            <% } else { %>
              <%= data[i].num_episodes %> Episodes
            <% } %>
          </span>
        </div>
      </div>
    </a>
<% } %>